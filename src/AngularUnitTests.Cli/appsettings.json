{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "AngularTestGenerator": {
    "TargetCoveragePercentage": 80,
    "TestFileExtension": ".spec.ts",
    "TypeScriptExtensions": [".ts"],
    "ExcludedDirectories": ["node_modules", "dist", ".angular", "coverage"],
    "GenerateJestConfig": true
  }
}
